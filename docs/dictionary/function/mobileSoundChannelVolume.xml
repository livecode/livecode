	<name>mobileSoundChannelVolume</name>
	<type>function</type>

	<syntax>
		<example>mobileSoundChannelVolume(<i>channel)</i></example>
	</syntax>

	<synonyms>
		<synonym>iphoneSoundChannelVolume</synonym>
