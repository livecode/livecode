	<name>mobileSoundOnChannel</name>
	<type>function</type>

	<syntax>
		<example>mobileSoundOnChannel(<i>channel)</i></example>
	</syntax>

	<synonyms>
		<synonym>iphoneSoundOnChannel</synonym>
