setup

test "abc" , "abc" is "abc,abc"

test "abc" , (1 + 2) is "abc,3"
test "abc" , (1 / 2) is "abc,0.5"
test "abc" , (1 > 2) is "abc,false"
test "abc" , empty is "abc,"
test empty , "abc" is ",abc"
//test ("abc",) is "abc,"

test 1 , (1 + 1) is "1,2"
test 1 , (1 / 2) is "1,0.5"
test 1 , (1 > 2) is "1,false"
test 1 , empty is "1,"

test empty , empty is ","

teardown