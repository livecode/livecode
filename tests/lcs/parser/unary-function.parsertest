%TEST UnaryFunctionNoParams
on parse_test
	get the fontlanguage %{AFTER_FUNCTION}
end parse_test
%EXPECT PASS
%ERROR PE_FACTOR_NOOF AT AFTER_FUNCTION
%ENDTEST

%TEST UnaryFunctionNoParams2
on parse_test
        get the hostnametoaddress %{AFTER_FUNCTION}
end parse_test
%EXPECT PASS
%ERROR PE_FACTOR_NOOF AT AFTER_FUNCTION
%ENDTEST

%TEST UnaryFunctionNoParamsParens
on parse_test
	get fontlanguage()%{AFTER_FUNCTION}
end parse_test
%EXPECT PASS
%ERROR PE_FONTLANGUAGE_BADPARAM AT AFTER_FUNCTION
%ENDTEST
