<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="lc-bootstrap-compile"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="lc-bootstrap-compile"
			DisplayName="lc-bootstrap-compile"
			CommandLine="mkdir $(SolutionDir)toolchain\lc-compile\generated\modules&#x0D;&#x0A;$(SolutionDir)_build\windows\$(ConfigurationName)\lc-bootstrap-compile.exe -modulepath $(SolutionDir)toolchain\lc-compile\generated\modules $(SolutionDir)engine\src\canvas.mlc&#x0D;&#x0A;$(SolutionDir)_build\windows\$(ConfigurationName)\lc-bootstrap-compile.exe -modulepath $(SolutionDir)toolchain\lc-compile\generated\modules $(SolutionDir)engine\src\engine.mlc&#x0D;&#x0A;$(SolutionDir)_build\windows\$(ConfigurationName)\lc-bootstrap-compile.exe -modulepath $(SolutionDir)toolchain\lc-compile\generated\modules $(SolutionDir)engine\src\widget.mlc&#x0D;&#x0A;$(SolutionDir)_build\windows\$(ConfigurationName)\lc-bootstrap-compile.exe -bootstrap -modulepath $(SolutionDir)toolchain\lc-compile\generated\modules -template &quot;$(SolutionDir)toolchain\lc-compile\src\grammar.g&quot; -output &quot;$(SolutionDir)toolchain\lc-compile\src\grammar_full.g&quot; &quot;$(SolutionDir)libscript\src\string.mlc&quot; &quot;$(SolutionDir)libscript\src\binary.mlc&quot; &quot;$(SolutionDir)libscript\src\arithmetic.mlc&quot; &quot;$(SolutionDir)libscript\src\bitwise.mlc&quot; &quot;$(SolutionDir)libscript\src\byte.mlc&quot; &quot;$(SolutionDir)libscript\src\char.mlc&quot; &quot;$(SolutionDir)libscript\src\list.mlc&quot; &quot;$(SolutionDir)libscript\src\type-convert.mlc&quot; &quot;$(SolutionDir)libscript\src\logic.mlc&quot; &quot;$(SolutionDir)libscript\src\sort.mlc&quot; &quot;$(SolutionDir)libscript\src\array.mlc&quot; &quot;$(SolutionDir)engine\src\canvas.mlc&quot; &quot;$(SolutionDir)engine\src\engine.mlc&quot;"
			Outputs="grammar_full.g"
			AdditionalDependencies="$(SolutionDir)libscript\src\*.mlc"
			FileExtensions="*.ignored"
			ExecutionDescription="lc-bootstrap-compile"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
